import {*} from "./assert"

fn fnWithSmallArg(x: Int<0, 16>) => {}
fn testGuard1(x: Int) => {
    if (x <= 16 && x >= 0) fnWithSmallArg(x)
}

fn testTruncate() => {
    let x: Int<0, 4294967295> = 0xffffffff
    assert.assert(x.truncateToSigned() == 0x7FFFFFFF)
}

export fn testInt() -> Void {
    testGuard1(23)
    testTruncate()
}